<h1>Smartech</h1>

<h2>Lista de productos</h2>
<div class="contenedorProductos">
    {{#each productos}}
        <div class="card">
            <p> {{this.title}} </p>
            <p> {{this.description}} </p>
            <p> {{this.price}} </p>
        </div>
    {{/each}}
</div>

<script src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/4.7.2/socket.io.min.js"></script>
<script src="js/main.js"></script>